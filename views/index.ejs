<% extend('template/layout') %>
<!--Loader-->
<div class="loader">
    <div class="cssload-container">
        <div class="cssload-circle"></div>
        <div class="cssload-circle"></div>
    </div>
</div>
<%= data[0].name%>
<!--Topbar-->
<div class="topbar">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <p class="pull-left hidden-xs">ตงตง ข้าวมันไก่</p>
                <p class="pull-right">
                    <i class="fa fa-phone"></i>
                    โทร 111-123-6789</p>
            </div>
        </div>
    </div>
</div>

<!--Header-->
<header id="main-navigation">
    <div id="navigation" data-spy="affix" data-offset-top="20">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <% include components/navbar.ejs %>
                </div>
            </div>
        </div>
    </header>

    <!-- REVOLUTION SLIDER -->

    <div
        id="rev_slider_34_1_wrapper"
        class="rev_slider_wrapper fullwidthbanner-container"
        data-alias="news-gallery34"
        style="margin:0px auto;background-color:#ffffff;padding:0px;margin-top:0px;margin-bottom:0px;">
        <!-- START REVOLUTION SLIDER 5.0.7 fullwidth mode -->
        <div
            id="rev_slider_34_1"
            class="rev_slider fullwidthabanner"
            style="display:none;"
            data-version="5.0.7">
            <ul>
                <!-- SLIDE  -->

                <% include components/slide1.ejs %>
                <% include components/slide2.ejs %>
                <% include components/slide3.ejs %>

                <!-- SLIDE  -->
            </ul>
        </div>
    </div>
    <!-- END REVOLUTION SLIDER -->

    <!--Features Section-->
    <section class="feature_wrap padding-half" id="specialities">
        <div class="container">
            <% include components/features/section1.ejs %>
        </div>

    </section>

    <!--Services plus working hours-->
    <section id="services" class="padding-bottom">
        <div class="container">
            <% include components/features/section2.ejs %>
        </div>
    </section>

    <!-- image with content -->
    <section class="info_section paralax">
        <div class="container">
            <% include components/features/section3.ejs %>
        </div>
    </section>

    <!-- Food Gallery -->
    <section id="gallery" class="padding">
        <div class="container">
            <% include components/features/section4.ejs %>
        </div>
    </section>

    <!-- facts counter  -->
    <section id="facts">
        <div class="container">
            <% include components/features/section5.ejs %>
        </div>
    </section>

    <!-- testinomial -->
    <section id="testinomial" class="padding">
        <div class="container">
            <% include components/features/section6.ejs %>
        </div>
    </section>

    <!--Footer-->
    <footer class="padding-top bg_black">
        <div class="container">
            <% include components/footer.ejs %>
        </div>
    </footer>

    <a href="#" id="back-top">
        <i class="fa fa-angle-up fa-2x"></i>
    </a>