<% for(var i=0; i< foodshot.length; i++) {%>
    <div class="col-1-4 mix work-item hot">
        <div class="wrap-col">
            <div class="item-container">
                <div class="image">
                    <img class="ds_fix_img"  src='<%=foodshot[i].image%>' alt="cook"/>
                    <div class="overlay">
                        <a
                            class="fancybox overlay-inner"
                            href='<%=foodshot[i].image%>'
                            data-fancybox-group="gallery">
                            <i class=" icon-eye6"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
<% } %>